{
  "name": "rename-text-layers-by-style",
  "description": "Rename all text layers based on the name of the text style assigned.",
  "version": "2.2.4",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "rename-text-layers-by-style",
    "manifest": "src/manifest.json",
    "main": "rename-text-layers-by-style.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okaybueno/rename-text-layers-by-style"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.7"
  },
  "author": "Alexander Gerund <alex@okaybueno.com>"
}
